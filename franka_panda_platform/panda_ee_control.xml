            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 1 0 0.5" size="0.005 1 0.001" type="box"/>
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0 1 0.5" size="0.005 0.005 1" type="box"/>
