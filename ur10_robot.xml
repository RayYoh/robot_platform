<mujoco model="ur10">
    <compiler angle="radian" meshdir="meshes/" coordinate="local" inertiafromgeom='auto' texturedir="textures"/>
    <option timestep="0.01" apirate="200" iterations="20" noslip_iterations="20" tolerance="1e-10" impratio="10"
			gravity="0 0 -9.81" solver="PGS" cone="elliptic" />
    <size njmax="1500" nconmax="500" nstack="5000000" />
    <visual>
        <map fogstart="3" fogend="5" force="0.1" znear="0.1"/>
        <quality shadowsize="4096" offsamples="8" />
        <global offwidth="3024" offheight="1680"/>
    </visual>
    <asset>
        <texture builtin="checker" height="512" name="texplane" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" type="2d" width="512"/>
        <material name="floorplane" reflectance="0.3" texrepeat="1 1" texture="texplane" texuniform="true"/>
    </asset>
    <asset>
        <mesh name="base" file="base.stl" />
        <mesh name="shoulder" file="shoulder.stl" />
        <mesh name="upperarm" file="upperarm.stl" />
        <mesh name="forearm" file="forearm.stl" />
        <mesh name="wrist1" file="wrist1.stl" />
        <mesh name="wrist2" file="wrist2.stl" />
        <mesh name="wrist3" file="wrist3.stl" />
        <mesh name="base_vis" file="base_vis.stl" />
        <mesh name="shoulder_vis" file="shoulder_vis.stl" />
        <mesh name="upperarm_vis" file="upperarm_vis.stl" />
        <mesh name="forearm_vis" file="forearm_vis.stl" />
        <mesh name="wrist1_vis" file="wrist1_vis.stl" />
        <mesh name="wrist2_vis" file="wrist2_vis.stl" />
        <mesh name="wrist3_vis" file="wrist3_vis.stl" />
    </asset>
    <default>
        <default class="ur10">
            <joint pos="0 0 0" axis="0 0 1" limited="true"/>
            <position ctrllimited="true" user="1002 40 2001 -0.005 0.005"/>
            <default class="ur10_vis">
                <geom contype="0" conaffinity="0" group="1" type="mesh" rgba=".7 .7 .7 1" mass="0"/>
            </default>
            <default class="ur10_col">
                <geom contype="1" conaffinity="1" group="2" type="mesh" rgba=".7 .7 .7 1"/>
            </default>
        </default>
    </default>
    <worldbody>
        <geom name="floor" pos="0. 0. 0" size="3 3 .125" type="plane" condim="4" material="floorplane" contype="1" conaffinity="1"></geom>
		<light directional="true" diffuse='1.0 1.0 1.0' pos="0 0 3" dir="0 0 -1"/>
        <geom class="ur10_vis" type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="base_vis" />
        <geom class="ur10_col" mesh="base"/>
        <body name="shoulder_link" pos="0 0 0.1273">
            <inertial pos="0 0 0" mass="7.778" diaginertia="0.0314743 0.0314743 0.0218756" />
            <joint name="shoulder_pan_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-6.28319 6.28319" damping="100" frictionloss="0.01"/>
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="shoulder_vis" />
            <geom class="ur10_col" mesh="shoulder"/>
            <body name="upper_arm_link" pos="0 0.220941 0" quat="0.707107 0 0.707107 0">
                <inertial pos="0 -0.045 0.306" mass="12.93" diaginertia="0.421754 0.421754 0.0363656" />
                <joint name="shoulder_lift_joint" pos="0 0 0" axis="0 1 0" limited="true" range="-6.28319 6.28319" damping="100" frictionloss="0.01"/>
                <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="upperarm_vis" />
                <geom class="ur10_col" mesh="upperarm"/>
                <body name="forearm_link" pos="0 -0.1719 0.612">
                    <inertial pos="0 0 0.28615" mass="3.87" diaginertia="0.11107 0.11107 0.0108844" />
                    <joint name="elbow_joint" pos="0 0 0" axis="0 1 0" limited="true" range="-3.14159 3.14159" damping="100" frictionloss="0.01"/>
                    <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="forearm_vis" />
                    <geom class="ur10_col" mesh="forearm"/>
                    <body name="wrist_1_link" pos="0 0 0.5723" quat="0.707107 0 0.707107 0">
                        <inertial pos="0 0.1149 0" quat="0.5 0.5 -0.5 0.5" mass="1.96" diaginertia="0.0055125 0.00510825 0.00510825" />
                        <joint name="wrist_1_joint" pos="0 0 0" axis="0 1 0" limited="true" range="-6.28319 6.28319" damping="100" frictionloss="0.01"/>
                        <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="wrist1_vis" />
                        <geom class="ur10_col" mesh="wrist1"/>
                        <body name="wrist_2_link" pos="0 0.1149 0">
                            <inertial pos="0 0 0.1157" quat="0.5 0.5 -0.5 0.5" mass="1.96" diaginertia="0.0055125 0.00510825 0.00510825" />
                            <joint name="wrist_2_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-6.28319 6.28319" damping="100" frictionloss="0.01"/>
                            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="wrist2_vis" />
                            <geom class="ur10_col" mesh="wrist2"/>
                            <body name="wrist_3_link" pos="0 0 0.1157">
                                <inertial pos="0 0.0769759 0" quat="0.707107 0 0 0.707107" mass="0.203" diaginertia="0.000204542 0.000117966 0.000117966" />
                                <joint name="wrist_3_joint" pos="0 0 0" axis="0 1 0" limited="true" range="-6.28319 6.28319" damping="100" frictionloss="0.01"/>
                                <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="wrist3_vis" />
                                <geom class="ur10_col" mesh="wrist3"/>
                                <geom size="0.005 0.005 0.005" pos="0 0.0822 0" quat="0.707107 0 0 0.707107" type="box" />
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <position name="shoulder_pan_joint" class="ur10" ctrlrange="-6.28319 6.28319" joint="shoulder_pan_joint" kp="870"/>
        <position name="shoulder_lift_joint" class="ur10" ctrlrange="-6.28319 6.28319" joint="shoulder_lift_joint" kp="870"/>
        <position name="elbow_joint" class="ur10" ctrlrange="-3.14159 3.14159" joint="elbow_joint" kp="870"/>
        <position name="wrist_1_joint" class="ur10" ctrlrange="-6.28319 6.28319" joint="wrist_1_joint" kp="120"/>
        <position name="wrist_2_joint" class="ur10" ctrlrange="-6.28319 6.28319" joint="wrist_2_joint" kp="120"/>
        <position name="wrist_3_joint" class="ur10" ctrlrange="-6.28319 6.28319" joint="wrist_3_joint" kp="120"/>
    </actuator>
</mujoco>
